<%- include('../partials/header') %>

<h2>Item list</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Location</th>
      <th>Date Added</th>

    </tr>
  </thead>
  <tbody>
    <!-- Write the line of EJS to iterate over movies using forEach -->
    <% items.forEach(function(i) { %>
      <tr>
        <td><%= i.name %></td>
        <td><%= i.type %></td>
        <td><%= i.location %></td>
        <td><%= i.dateAdded %></td>
      </tr>
    <!-- Close the forEach using EJS here -->
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>